//-корзинка в шапке
mixin cart
	i.cart&attributes(attributes)
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="23" height="21" viewBox="0 0 23 21")
			image(id="cart.svg" width="23" height="21" xlink:href="data:img/png;base64,")

//-корзинка на кнопке
mixin cart-in-button
	i.cart-in-button&attributes(attributes)
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="22" height="20" viewBox="0 0 22 20")
			image(id="btn-cart.svg" width="22" height="20" xlink:href="data:img/png;base64,")

//-местоположение
mixin maps-icon
	i.maps-icon&attributes(attributes)
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="11" height="16" viewBox="0 0 11 16")
			image(id="maps.svg" width="11" height="16" xlink:href="data:img/png;base64,")

//-местоположение
mixin fire-icon
	i.fire-icon&attributes(attributes)
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="25" height="31" viewBox="0 0 25 31")
			image(id="fire.svg" width="25" height="31" xlink:href="data:img/png;base64,")

//- стрелка в пагинации

mixin arr-right
	i.arr-right
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="27" height="20" viewBox="0 0 27 20")
			image(width="27" height="20" xlink:href="data:img/png;base64,")
mixin arr-left
	i.arr-right.arr-right--rotate
		svg(xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="27" height="20" viewBox="0 0 27 20")
			image( width="27" height="20" xlink:href="data:img/png;base64,")


